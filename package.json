{
  "name": "levelup2",
  "version": "1.0.0",
  "description": "second version of levelup project",
  "scripts": {
    "start": "cross-env NODE_PATH=./app concurrently --kill-others \"nodemon app/server\" \"webpack-dev-server --progress\"",
    "test": "cross-env NODE_ENV=test cross-env NODE_PATH=./app mocha --compilers js:babel-register -r app/specs/support/setup.mocha.js --recursive \"app/specs/**/*.test.js\" -w",
    "webpack": "webpack",
    "build": "rimraf dist/ && npm run webpack && gulp build",
    "prod": "rimraf dist/ && cross-env NODE_ENV=production npm run webpack && gulp build",
    "postinstall": "npm run build"
  },
  "author": "nooblisk",
  "license": "MIT",
